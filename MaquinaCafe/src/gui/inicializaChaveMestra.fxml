<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="ADICIONA CHAVE-MESTRA" BorderPane.alignment="CENTER" />
    </top>
    <center>
        <GridPane alignment="CENTER" hgap="10" vgap="10">
            <Label text="Username" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="usernameLogin" editable="false" text="adm" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Label text="Senha" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="senhaChaveMestra" GridPane.columnIndex="1" GridPane.rowIndex="1"  />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </center>
    <bottom>
        <Button alignment="CENTER_RIGHT" onAction="#inicializaChaveMestra" text="Finish" BorderPane.alignment="CENTER_RIGHT" />
    </bottom>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10" />
    </padding>
</BorderPane>
